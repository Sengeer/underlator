{
  "name": "analytical-platform",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://sengeer.github.io/offline-pdf-translator/",
  "dependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.22.2",
    "react-scripts": "^5.0.1",
    "sass": "^1.70.0",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint . --ext .js,.jsx .",
    "lint:fix": "eslint --fix . --ext .jsx",
    "stylelint": "stylelint '**/*{.css,.scss}'",
    "stylelint:fix": "stylelint '**/*{.css,.scss}' --fix",
    "format": "prettier . --write",
    "lint-staged": "lint-staged",
    "prepare": "cd ../ && husky ./react-app/.husky",
    "jsdoc": "npx jsdoc -c .jsdoc.json",
    "electron": "npm run build && node build-electron.mjs && cd ../electron && npm run make"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@feature-sliced/eslint-config": "^0.1.1",
    "cpy": "^11.1.0",
    "eslint": "^8.57.0",
    "eslint-plugin-boundaries": "^4.2.2",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-react": "^7.34.0",
    "husky": "^9.1.4",
    "jsdoc": "^4.0.3",
    "lint-staged": "^15.2.10",
    "prettier": "^3.2.5",
    "rimraf": "^6.0.1",
    "stylelint": "^16.8.2",
    "stylelint-config-standard-scss": "^13.1.0",
    "stylelint-order": "^6.0.4",
    "stylelint-prettier": "^5.0.2",
    "stylelint-scss": "^6.5.0",
    "stylelint-selector-bem-pattern": "^4.0.1"
  },
  "lint-staged": {
    "*.{js,jsx}": "npm run lint:fix",
    "*{.css,.scss}": "npm run stylelint:fix",
    "*.{js,jsx,scss,md,html,json}": "npm run format"
  }
}
